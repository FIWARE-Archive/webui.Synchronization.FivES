<?xml version="1.0" encoding="UTF-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
                   assembly="Core"
                   namespace="FIVES">
	<class name="ComponentRegistry" lazy="false">
		<id name="Id" type = "Guid">
			<generator class = "guid" />
		</id>
		<map name="registryHandler" table = "compinfo_in_registries" cascade = "all">
		  <key column = "registryId" />
		  <index type = "string" column = "registryName" />
		  <many-to-many column = "layoutID" class="ComponentInfo" />
		</map>
	</class>
</hibernate-mapping>

