<?xml version="1.0" encoding="UTF-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
                   assembly="DomainModel"
                   namespace="FIVES">
    <class name="Entity" lazy="false">
      <id name="Guid" type="Guid" />
      <map name="ComponentsDictionaryHandler" table="entityComponents" cascade="all">
        <key column="EntityGuid" />
        <index column="ComponentName" type="System.String" />
        <many-to-many class="Component" />
      </map>
    </class>
</hibernate-mapping>
